#include <stdio.h>
int main() {
    int a[5], b[10], i;

    printf("Enter 5 elements:\n");
    for(i = 0; i < 5; i++) {
        scanf("%d", &a[i]);
    }

    for(i = 0; i < 5; i++) {
        b[i] = a[i];
    }

    for(i = 5; i < 10; i++) {
        b[i] = 0;
    }

    printf("Array B (10 elements):\n");
    for(i = 0; i < 10; i++) {
        printf("%d ", b[i]);
    }
    printf("\n");

    return 0;
}
